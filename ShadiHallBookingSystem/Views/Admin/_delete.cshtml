@model ShadiHallBookingSystem.Models.Admin


@*<form method="post">*@
@using (Html.BeginForm("Delete", "Admin", FormMethod.Post))
{
    @Html.HiddenFor(a => a.A_ID)
    <div class="form-group">
        <label>Email:</label><span>@Model.Email</span>
    </div>
    <div class="form-group">
        <label>Password:</label><span>@Model.Password</span>

    </div>
   

    <div class="form-group">
        <button class="btn btn-danger" type="submit"><span class="glyphicon glyphicon-trash"></span>&nbsp; Yes</button>
        <button class="btn btn-success" type="button" data-dismiss="modal"><span class="glyphicon glyphicon-circle-arrow-left"></span>&nbsp; No</button>
    </div>
}
@*</form>*@
