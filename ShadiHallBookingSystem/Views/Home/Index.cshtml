    <script>
        window.onload = function () {


            var charge = $("#charge").val();
            var message = $("#message").val();
            
           
            if (charge.length > 1)

            {

                sweetAlert(
                    {
                        title: "CONFIRMATION OF BOOKING",
                        text: "Your booking is done successfully ! And we have got your amount of "+" " + charge + " " + " rupees",
                        icon: "success",
                        
                        //confirmButtonColor: "#DD6B55",
                        confirmButtonText: "OK!"
                    }
                );
                if (message.length > 1)
                    sweetAlert(
                        {
                            title: "CONFIRMATION OF BOOKING",
                            text: message,
                            icon: "error",
                        });


               
            }
           

        };
    </script>
